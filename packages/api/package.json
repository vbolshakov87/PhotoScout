{
  "name": "@photoscout/api",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "esbuild src/handlers/*.ts --bundle --platform=node --target=node20 --outdir=dist --format=esm --external:@aws-sdk/* --banner:js=\"import { createRequire } from 'module'; const require = createRequire(import.meta.url);\" && echo '{\"type\":\"module\"}' > dist/package.json",
    "dev": "tsx watch src/dev-server.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:prompt": "vitest run src/tests/prompt.test.ts --config vitest.integration.config.ts",
    "test:llm": "vitest run src/tests/llm-comparison.test.ts --config vitest.integration.config.ts",
    "test:llm:haiku": "CLAUDE_MODEL=haiku vitest run src/tests/llm-comparison.test.ts --config vitest.integration.config.ts",
    "test:llm:sonnet": "CLAUDE_MODEL=sonnet vitest run src/tests/llm-comparison.test.ts --config vitest.integration.config.ts",
    "test:security": "vitest run src/tests/security/security.test.ts --config vitest.integration.config.ts",
    "test:security:quick": "vitest run src/tests/security/security.test.ts --config vitest.integration.config.ts --testNamePattern='Quick'",
    "test:security:critical": "vitest run src/tests/security/security.test.ts --config vitest.integration.config.ts --testNamePattern='Critical'",
    "typecheck": "tsc --noEmit",
    "security:convert": "tsx scripts/convert-security-to-promptfoo.ts",
    "security:eval": "npx promptfoo@latest eval --env-file ../../.env --max-concurrency 2",
    "security:eval:fast": "npx promptfoo@latest eval --env-file ../../.env --max-concurrency 4",
    "security:eval:critical": "npx promptfoo@latest eval --env-file ../../.env --filter-pattern CRITICAL --max-concurrency 2",
    "security:eval:injection": "npx promptfoo@latest eval --env-file ../../.env --filter-pattern prompt_injection --max-concurrency 2",
    "security:eval:jailbreak": "npx promptfoo@latest eval --env-file ../../.env --filter-pattern jailbreak --max-concurrency 2",
    "security:report": "npx promptfoo@latest eval --env-file ../../.env --output security-report.html --max-concurrency 2",
    "security:view": "npx promptfoo@latest view",
    "quality:convert": "tsx scripts/convert-quality-to-promptfoo.ts",
    "quality:eval": "npx promptfoo@latest eval --config promptfoo-quality.yaml --env-file ../../.env --max-concurrency 2",
    "quality:eval:fast": "npx promptfoo@latest eval --config promptfoo-quality.yaml --env-file ../../.env --max-concurrency 4",
    "quality:report": "npx promptfoo@latest eval --config promptfoo-quality.yaml --env-file ../../.env --output quality-report.html --max-concurrency 2",
    "quality:view": "npx promptfoo@latest view --config promptfoo-quality.yaml",
    "images:fetch": "tsx scripts/fetch-destination-images.ts",
    "images:fetch:dry-run": "tsx scripts/fetch-destination-images.ts --dry-run",
    "images:fetch:test": "tsx scripts/fetch-destination-images.ts --limit 3 --dry-run"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-dynamodb": "^3.972.0",
    "@aws-sdk/client-s3": "^3.972.0",
    "@aws-sdk/lib-dynamodb": "^3.972.0",
    "@photoscout/shared": "workspace:*",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "openai": "^6.16.0",
    "unsplash-js": "^7.0.20",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/node": "^25.0.10",
    "@types/uuid": "^11.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "esbuild": "^0.27.2",
    "promptfoo": "^0.120.17",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18",
    "yaml": "^2.7.0"
  }
}
